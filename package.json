{
  "name": "cyber-survivor-ts",
  "version": "0.5.0",
  "private": true,
  "description": "CyberSurvivor game (Web / Vite + TypeScript)",
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:renderer": "vite build",
    "build": "vite build",
    "build:clean": "npm run clean:all && vite build",
    "preview": "vite preview",
    "backend": "echo backend removed",
    "clean": "rimraf dist",
    "clean:all": "rimraf dist node_modules/.vite node_modules/.cache .vite",
    "version:patch": "npm version patch -m 'chore(release): bump version to %s'",
    "postinstall": "echo (no electron postinstall)",
    "test": "vitest run",
    "sim": "node ./scripts/run-sim.cjs --sec=45 --seeds=1,2,3",
    "sim:log": "node ./scripts/run-sim.cjs --sec=45 --seeds=1,2,3 --log=1 --logdir=sim-logs",
    "sim:log:short": "node ./scripts/run-sim.cjs --sec=10 --seeds=1,2 --log=1 --logdir=sim-logs-test",
    "sim:summary": "node ./scripts/summarize-logs.cjs --dir=sim-logs",
    "sim:summary:json": "node ./scripts/summarize-logs.cjs --dir=sim-logs --json=1 --out=sim-logs-summary.json",
    "sim:summary:compare": "node ./scripts/compare-summaries.cjs --old=baseline-summary.json --new=sim-logs-summary.json --failKills=2",
    "sim:full": "node ./scripts/run-sim.cjs --sec=1200 --seeds=1,2,3 --log=1 --logdir=sim-logs-full",
    "sim:full:summary": "node ./scripts/summarize-logs.cjs --dir=sim-logs-full --json=1 --out=sim-logs-full-summary.json",
    "sim:full:compare": "node ./scripts/compare-summaries.cjs --old=baseline-summary.json --new=sim-logs-full-summary.json --failKills=2",
    "sim:30s:loadouts": "node ./scripts/run-sim.cjs --sec=30 --seeds=1,2,3 --mode=NORMAL --loadout=scripts/loadouts.example.json --out=loadouts-30s-summary.json",
    "sim:30s:pressure": "node ./scripts/run-sim.cjs --sec=30 --seeds=1,2,3 --mode=NORMAL --pressure=enabled:1,spawnRateMul:2,packSize:10,hpMul:1.4,speedMul:1.2,rampPerMin:0.5,eliteChance:0.1 --out=pressure-30s-summary.json",
    "sim:snapshots": "node ./scripts/run-sim.cjs --sec=60 --seeds=1,2,3 --mode=NORMAL --pressure=enabled:1,rampPerMin:0.6,spawnRateMul:2,packSize:10,hpMul:1.4,speedMul:1.2 --minutes=15,20,25,30 --out=snapshots-60s-summary.json",
    "sim:power:10s": "node ./scripts/run-sim.cjs --sec=10 --seeds=1,2,3 --power=1 --mode=NORMAL --out=power-10s.json",
    "sim:power:30s": "node ./scripts/run-sim.cjs --sec=30 --seeds=1,2,3 --power=1 --mode=NORMAL --out=power-30s.json",
    "sim:power:pressure": "node ./scripts/run-sim.cjs --sec=30 --seeds=1,2,3 --power=1 --mode=NORMAL --pressure=enabled:1,spawnRateMul:2,packSize:10,hpMul:1.4,speedMul:1.2,rampPerMin:0.5,eliteChance:0.1 --out=power-pressure-30s.json",
    "sim:power:snapshots": "node ./scripts/run-sim.cjs --sec=60 --seeds=1,2,3 --power=1 --mode=NORMAL --pressure=enabled:1,rampPerMin:0.6,spawnRateMul:2,packSize:10,hpMul:1.4,speedMul:1.2 --minutes=15,20,25,30 --out=power-snapshots-60s.json",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "lint:security": "echo (placeholder for future security lint)",
    "pf": "node .\\scripts\\power-factor.cjs --level=7 --timeMin=15 --out=pf-output.json",
    "pf:l1": "node .\\scripts\\power-factor.cjs --level=1 --timeMin=5 --out=pf-l1.json",
    "pf:l7": "node .\\scripts\\power-factor.cjs --level=7 --timeMin=25 --out=pf-l7.json",
    "assets:generate:elites": "node .\\scripts\\generate-elites.cjs",
    "assets:clean:elites": "node .\\scripts\\cleanup-elites.cjs"
  },
  "build": {},
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@resvg/resvg-js": "^2.6.2",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/howler": "^2.2.12",
    "@types/node": "^20.0.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "postcss": "^8.5.6",
    "rimraf": "^5.0.5",
    "tailwindcss": "^4.1.13",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-aria/visually-hidden": "^3.8.27",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.23.12",
    "howler": "^2.2.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  }
}
